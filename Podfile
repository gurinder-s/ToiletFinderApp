platform :ios, '12.0'
workspace 'ToiletFinderWorkspace.xcworkspace'

target 'ToiletFinder' do
  project 'ToiletFinder/ToiletFinder.xcodeproj'
  use_frameworks!
  pod 'Firebase/Auth'
  pod 'Firebase/Firestore'
end

target 'ToiletFinderServices' do
  project 'ToiletFinderServices/ToiletFinderServices.xcodeproj'
  use_frameworks!
  pod 'Firebase/Auth'
  pod 'Firebase/Firestore'
end
